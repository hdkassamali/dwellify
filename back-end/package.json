{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "migration:create-db": "node ./database/create-database.js",
    "migration:generate-and-run": "npm run migration:generate && npm run migration:run",
    "migration:generate": "typeorm migration:generate -d ./database/data-source -o ./database/migrations/migrations",
    "migration:run": "typeorm migration:run -d ./database/data-source.js",
    "migration:revert": "typeorm migration:revert -d ./database/data-source.js",
    "migration:session-table": "node ./database/migrations/session-table.js",
    "seed": "node ./database/seed.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "connect-pg-simple": "^9.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "~4.16.1",
    "express-session": "^1.18.1",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "main": "app.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.1",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.11",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.2"
  }
}
